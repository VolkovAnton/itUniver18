@{
    ViewBag.Title = "Home Page";
}
@model IEnumerable<WebApplication1.Models.Document>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h3>Хранилище данных</h3>
    <table>
        <tr class="header">
            <td><p>Название файла</p></td>
            <td><p>Автор</p></td>
        </tr>
        @foreach (WebApplication1.Models.Document b in Model)
        {
            <tr>
                <td><p>@b.Name</p></td>
                <td><p>@b.Author</p></td>
                <td><p><a href="@b.Id"></a></p></td>
            </tr>
        }
    </table>
</div>


